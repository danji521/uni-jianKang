.header {
  position: relative;
  height: 416rpx;
  background-image: -webkit-linear-gradient(top, #00cfb7 0%, #26dca2 100%);
  background-image: linear-gradient(180deg, #00cfb7 0%, #26dca2 100%);
}
.header .o-round {
  position: absolute;
  left: 50%;
  top: 40rpx;
  margin-left: -184rpx;
  width: 368rpx;
  height: 368rpx;
  text-align: center;
  border-radius: 50%;
  border: 10rpx solid #65e5cc;
}
.header .o-round::after {
  content: "";
  display: block;
  position: absolute;
  left: -4px;
  top: -4px;
  width: 157px;
  height: 157px;
  text-align: center;
  border-radius: 50%;
  border: 4px solid #FFFFFF;
  border-bottom: 0px;
}
.header .w-round {
  position: relative;
  top: 80rpx;
  left: 50%;
  margin-left: -141rpx;
  width: 282rpx;
  height: 282rpx;
  text-align: center;
  border-radius: 50%;
  border: 10rpx dotted #FFFFFF;
  z-index: 1;
}
.header .w-round :nth-child(1) {
  padding-top: 46rpx;
  font-size: 34rpx;
  color: #FFFFFF;
}
.header .w-round :nth-child(2) {
  padding-top: 40rpx;
  font-size: 66rpx;
  color: #fffa98;
}
.header .w-round :nth-child(3) {
  position: relative;
  left: -13rpx;
  padding-bottom: 25rpx;
  width: 300rpx;
  height: 52rpx;
  font-size: 20rpx;
  color: #FFFFFF;
  background-color: #1fd9a5;
  z-index: 10;
}
.header::after {
  position: absolute;
  bottom: -13rpx;
  content: "";
  display: block;
  width: 100%;
  height: 70rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  -webkit-animation: mymove 5s steps(8) infinite;
          animation: mymove 5s steps(8) infinite;
  z-index: 5;
}
@-webkit-keyframes mymove {
0% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
100% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
}
@keyframes mymove {
0% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
100% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
}
.nav {
  margin: 0 40rpx;
  margin-top: 20rpx;
  padding-left: 24rpx;
  line-height: 114rpx;
  font-size: 40rpx;
  color: #0bc1b5;
  border-radius: 20rpx 20rpx 0 0;
  background-color: #d4f4f1;
}
.weight {
  margin-top: 60rpx;
  padding: 0 60rpx;
  height: 102rpx;
}
.weight .weight-hd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 40rpx;
  font-size: 24rpx;
}
.weight .weight-hd :nth-child(1) {
  margin-top: 20rpx;
}
.weight .weight-hd :nth-child(2) {
  margin-left: 56rpx;
  font-size: 40rpx;
}
.weight .weight-bd {
  padding-left: 70rpx;
  margin-top: 40rpx;
  font-size: 20rpx;
}
.weight .weight-bd image {
  width: 20rpx;
  height: 20rpx;
}
.weight .weight-bd .large {
  margin-right: 14rpx;
  font-size: 46rpx;
  color: #00d1ad;
}
.bmi {
  margin-top: 80rpx;
  padding: 0 60rpx;
  font-size: 20rpx;
}
.bmi .bmi-title {
  font-weight: 600;
}
.bmi .bmi-surface {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
  margin-top: 60rpx;
  height: 22rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.bmi .bmi-surface view {
  margin-top: 60rpx;
  font-size: 20rpx;
  color: #999999;
}
.bmi .bmi-surface::after {
  content: "23.2";
  display: block;
  position: absolute;
  bottom: 40rpx;
  left: 30%;
  width: 80rpx;
  line-height: 40rpx;
  text-align: center;
  color: #FFFFFF;
  border-radius: 10rpx;
  background-color: #00ba7f;
}
.bmi .bmi-surface::before {
  content: "";
  display: block;
  position: absolute;
  top: -20rpx;
  left: 30%;
  margin-left: 30rpx;
  width: 0;
  height: 0;
  border-left: 6rpx solid transparent;
  border-right: 6rpx solid transparent;
  border-top: 15rpx solid #00ba7f;
}
.bmi .bmi-result {
  margin-top: 60rpx;
  padding: 0 24px;
  line-height: 60rpx;
  color: #d6d7db;
  background-color: #f4f8fb;
  border-radius: 10rpx;
}
.the-waist {
  margin-top: 106rpx;
  padding-left: 60rpx;
  padding-bottom: 100rpx;
  height: 126rpx;
  font-size: 30rpx;
}
.the-waist :nth-child(2) {
  margin-top: 34rpx;
  width: 126rpx;
  line-height: 66rpx;
  text-align: center;
  color: #10ceac;
  border-radius: 20rpx;
  background-color: #d6f3ef;
}

