.amusing {
  padding: 40rpx;
}
.rewards {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx;
  height: 134rpx;
  border-radius: 20rpx;
  background-color: #fde8e3;
  box-sizing: border-box;
}
.rewards .rewards-coins,
.rewards .rewards-money {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 50%;
  height: 100%;
  font-size: 20rpx;
  color: #99908b;
  z-index: 1;
}
.rewards .rewards-coins :nth-child(1),
.rewards .rewards-money :nth-child(1) {
  font-size: 32rpx;
  color: #000000;
}
.rewards .rewards-coins .rewards-money_hint,
.rewards .rewards-money .rewards-money_hint {
  position: absolute;
  top: 26rpx;
  right: 80rpx;
  line-height: 30rpx;
  padding: 0 8rpx;
  color: #fe6470;
  font-size: 18rpx;
  background-color: #fcf26e;
  border-radius: 8rpx;
  z-index: -1;
}
.rewards .rewards-coins .rewards-money_hint::after,
.rewards .rewards-money .rewards-money_hint::after {
  display: block;
  position: absolute;
  top: 10rpx;
  left: -6rpx;
  content: "";
  width: 8rpx;
  height: 8rpx;
  background-color: #fcf26e;
  -webkit-transform: rotate(-45deg);
          transform: rotate(-45deg);
}
.rewards .rewards-coins {
  margin-right: 40rpx;
  border-right: 2rpx solid #f8e4dd;
}
.rewards image {
  margin-right: 24rpx;
  width: 54rpx;
  height: 54rpx;
}
.containers1 {
  position: relative;
  margin-top: 34rpx;
  padding: 0 30rpx;
  height: 312rpx;
  border: 3rpx solid #f7f7f7;
  border-radius: 20rpx;
  overflow: hidden;
}
.containers1 .containers1-users {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 30rpx;
  font-size: 24rpx;
  color: #bebdc2;
}
.containers1 .containers1-users image {
  margin-right: 20rpx;
  width: 54rpx;
  height: 54rpx;
  border-radius: 50%;
  vertical-align: bottom;
}
.containers1 .containers1-users .containers1-users-ul {
  width: 370rpx;
  height: 60rpx;
  overflow: hidden;
}
.containers1 .containers1-plans {
  margin-top: 42rpx;
  line-height: 34rpx;
  font-size: 34rpx;
}
.containers1 .containers1-rewards {
  margin-top: 22rpx;
  line-height: 22rpx;
  font-size: 22rpx;
  color: #acabb0;
}
.containers1 .containers1-tags {
  position: absolute;
  right: 0;
  top: 30rpx;
  width: 80rpx;
  line-height: 44rpx;
  font-size: 20rpx;
  background-color: #f4f5f7;
  text-align: center;
  border-bottom-left-radius: 26rpx;
  border-top-left-radius: 26rpx;
}
.containers1 .containers1-but {
  position: absolute;
  margin-top: 10rpx;
  right: 42rpx;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  border: 2rpx solid #C8C7CC;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  z-index: 1;
}
.containers1::after {
  position: absolute;
  left: 0rpx;
  bottom: -48rpx;
  display: block;
  content: "";
  width: 105%;
  height: 100rpx;
  background-color: #ddf1f8;
  border-top-right-radius: 80rpx;
  -webkit-transform: rotate(-6deg);
          transform: rotate(-6deg);
}

