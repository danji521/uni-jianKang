.planDetails {
  padding-bottom: 40rpx;
}
.header {
  position: relative;
  width: 100%;
  height: 416rpx;
  text-align: center;
  font-size: 26rpx;
  color: #FFFFFF;
  box-sizing: border-box;
  background-image: -webkit-linear-gradient(top, #00cfb7 0%, #26dca2 100%);
  background-image: linear-gradient(180deg, #00cfb7 0%, #26dca2 100%);
}
.header :nth-child(1) {
  padding-top: 66rpx;
  font-size: 40rpx;
}
.header :nth-child(2) {
  position: relative;
  margin: 26rpx auto;
  width: 0;
  height: 0;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-top: 20rpx solid #e6ed9f;
}
.header :nth-child(2)::after {
  position: absolute;
  content: "";
  bottom: 0rpx;
  left: 20rpx;
  width: 0;
  height: 0;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-top: 20rpx solid #e6ed9f;
}
.header :nth-child(2)::before {
  position: absolute;
  content: "";
  bottom: 0rpx;
  right: 20rpx;
  width: 0;
  height: 0;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-top: 20rpx solid #e6ed9f;
}
.header::after {
  position: absolute;
  bottom: 0;
  content: "";
  display: block;
  width: 100%;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  -webkit-animation: mymove 6s steps(8) infinite;
          animation: mymove 6s steps(8) infinite;
}
@-webkit-keyframes mymove {
0% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
100% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
}
@keyframes mymove {
0% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
100% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
}
.nav {
  margin: 0 50rpx;
  margin-top: 20rpx;
  height: 196rpx;
  font-size: 20rpx;
  padding-left: 106rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  border: 2rpx solid #C0C0C0;
  box-sizing: border-box;
}
.nav .nav-li {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.nav .nav-li view {
  margin-top: 20rpx;
}
.nav .nav-li view:nth-child(1) {
  width: 160rpx;
}
.plan {
  margin: 0 50rpx;
  margin-top: 66rpx;
  height: 188rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  border: 2rpx solid #C0C0C0;
}
.plan .plan-hd,
.plan .plan-bd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 100rpx;
}
.plan .plan-hd {
  padding-top: 40rpx;
  font-size: 40rpx;
}
.plan .plan-bd {
  margin-top: 30rpx;
  font-size: 20rpx;
  color: #a4a4a4;
}
.users {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 30rpx;
  padding-left: 50rpx;
  margin-top: 34rpx;
  font-size: 24rpx;
}
.users .users-imgs image {
  margin-left: -10rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  overflow: hidden;
}
.footer {
  padding: 0 40rpx;
  height: 370rpx;
  margin: 30rpx;
  font-size: 28rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  border-radius: 20rpx;
  box-sizing: border-box;
}
.footer view {
  margin-top: 20rpx;
}
.footer :nth-child(1) {
  padding-top: 34rpx;
  font-size: 40rpx;
  color: #f68f62;
}

