.order {
  position: relative;
  height: 100vh;
  font-size: 28rpx;
  background-color: #fafafa;
  border-top: 20rpx solid transparent;
  -webkit-border-image: url(data:image/png;base64,) repeat;
          border-image: url(data:image/png;base64,) repeat;
}
.address {
  padding: 36rpx 30rpx;
  background-color: #FFFFFF;
}
.products {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top: 24rpx;
  padding: 36rpx 30rpx;
  height: 204rpx;
  background-color: #FFFFFF;
}
.products image {
  width: 126rpx;
  height: 126rpx;
}
.products .title {
  font-size: 30rpx;
}
.products .title :nth-child(1) {
  position: absolute;
  right: 20rpx;
  color: #b5b5b5;
}
.products .title :nth-child(2) {
  margin-top: 10rpx;
  font-size: 20rpx;
  color: #b5b5b5;
}
.products .title :nth-child(3) {
  margin-top: 10rpx;
  color: #fb683c;
}
.message {
  margin-top: 24rpx;
  padding: 24rpx 30rpx;
  font-size: 24rpx;
  background-color: #FFFFFF;
}
.message textarea {
  font-size: 24rpx;
}

