.emotion {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 0 68rpx;
  margin-top: 54rpx;
  height: 88rpx;
  border-radius: 20rpx;
  border: 2rpx solid #C8C7CC;
}
.emotion .emotion-look {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.emotion .emotion-look image {
  position: relative;
  top: 0rpx;
  left: -14rpx;
  margin-right: 10rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}
.emotion .emotion-reset {
  margin-right: 20rpx;
  width: 76rpx;
  line-height: 48rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 20rpx;
  background-color: #04cfbc;
  border-radius: 20rpx;
}
.nav .tab-hd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 32rpx;
  font-size: 36rpx;
  color: #a6a4a9;
  margin-top: 60rpx;
}
.nav .tab-hd .activ {
  position: relative;
}
.nav .tab-hd .activ::after {
  position: absolute;
  bottom: -10rpx;
  left: 50%;
  margin-left: -25rpx;
  content: "";
  width: 50rpx;
  height: 10rpx;
  color: red;
  background-color: red;
}
.nav .tab-bd-list {
  margin: 40rpx;
  padding-left: 36rpx;
  height: 154rpx;
  border-radius: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  border: 2rpx solid #cccccc;
  box-sizing: border-box;
}
.nav .tab-bd-list :nth-child(1) {
  padding-top: 40rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.nav .tab-bd-list :nth-child(2) {
  margin-top: 20rpx;
  font-size: 20rpx;
  color: #c4c4c6;
}

