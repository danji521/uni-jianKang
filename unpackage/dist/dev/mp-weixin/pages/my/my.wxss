.header {
  position: relative;
  height: 416rpx;
  background-image: -webkit-linear-gradient(top, #00cfb7 0%, #26dca2 100%);
  background-image: linear-gradient(180deg, #00cfb7 0%, #26dca2 100%);
}
.header .logo {
  position: relative;
  left: 50%;
  top: 40rpx;
  margin-left: -35rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  border: 2rpx solid #FFFFFF;
}
.header .phone {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin-top: 55rpx;
  line-height: 40rpx;
  font-size: 40rpx;
  color: #FFFFFF;
}
.header .phone :nth-child(2) {
  margin-left: 12rpx;
  width: 134rpx;
  text-align: center;
  line-height: 34rpx;
  font-size: 20rpx;
  background-color: #FFFFFF;
  color: #1dd7a6;
  border-radius: 15rpx;
}
.header .gender,
.header .birthday {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 46rpx;
  margin-top: 20rpx;
  color: #FFFFFF;
  font-size: 28rpx;
  line-height: 28rpx;
}
.header .gender image,
.header .birthday image {
  margin-right: 10rpx;
  width: 28rpx;
  height: 28rpx;
  vertical-align: bottom;
}
.header .birthday {
  margin-top: 24rpx;
}
.header::after {
  position: absolute;
  bottom: 0;
  content: "";
  display: block;
  width: 100%;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  -webkit-animation: mymove 6s steps(8) infinite;
          animation: mymove 6s steps(8) infinite;
}
@-webkit-keyframes mymove {
0% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
100% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
}
@keyframes mymove {
0% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
100% {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
}
.nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 148rpx;
  border-bottom: 2rpx solid #fafafa;
}
.nav image {
  width: 58rpx;
  height: 58rpx;
}
.nav .report,
.nav .scheme,
.nav .diet,
.nav .motion {
  position: relative;
  width: 25%;
  text-align: center;
  font-size: 24rpx;
}
.nav .report view:nth-child(1),
.nav .scheme view:nth-child(1),
.nav .diet view:nth-child(1),
.nav .motion view:nth-child(1) {
  height: 58rpx;
  line-height: 58rpx;
  border-left: 2rpx solid #a9a9a9;
}
.nav .report :nth-child(2),
.nav .scheme :nth-child(2),
.nav .diet :nth-child(2),
.nav .motion :nth-child(2) {
  margin-top: 14rpx;
}
.nav .report :nth-child(1) {
  color: #0bc8b2;
  font-size: 40rpx;
  font-weight: 600;
  border-left: 0 !important;
}
.nav .report :nth-child(1)::after {
  position: absolute;
  top: 8rpx;
  margin-left: 4rpx;
  content: "分";
  font-size: 20rpx;
  color: #a9a9a9;
}
.human {
  position: relative;
}
.human image {
  position: relative;
  left: 50%;
  top: 32rpx;
  margin-left: -78rpx;
  width: 156rpx;
  height: 390rpx;
}
.human .human-weight {
  position: absolute;
  top: 120rpx;
  left: 50%;
