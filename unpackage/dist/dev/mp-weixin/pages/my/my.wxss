  color: #2adfa8;
  font-size: 24rpx;
  border: 2rpx solid #ededed;
  border-radius: 10rpx;
}
.human .human-roughage::after {
  position: absolute;
  top: 37rpx;
  left: 20rpx;
  content: "";
  width: 14rpx;
  height: 20rpx;
  border-right: 3rpx solid #ededed;
  border-bottom: 3rpx solid #ededed;
